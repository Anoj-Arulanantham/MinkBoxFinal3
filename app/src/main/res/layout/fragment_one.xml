<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anojarulanantham.minkboxfinal.OneFragment">

    <ImageView
        android:id="@+id/gallery"
        android:layout_below="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/pushImage"
        app:srcCompat="@drawable/gallery64" />

    <ImageView
        android:layout_width="208dp"
        android:layout_height="350dp"
        android:src="@drawable/harder"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/imageView"
        android:scaleType="fitXY"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="10dp" />

    <Button
        android:text="Push Image"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:id="@+id/pushImage"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/camera"
        android:layout_marginTop="5dp" />

    <ImageView
        android:id="@+id/camera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/pushImage"
        android:layout_below="@id/imageView"
        app:srcCompat="@drawable/camera64" />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="16dp"
        android:orientation="vertical"
        tools:context="com.blogspot.android_er.androidclient.OneFragment">

    </LinearLayout>

</RelativeLayout>